# example.env

# Discord Bot Configuration
VOID_TOKEN=your_discord_bot_token_here
VOID_ID=your_discord_bot_id_here
APP_ID=your_discord_app_id_here

# API Keys
GEMINI_API_KEY=your_gemini_api_key_here
GROQ_API_KEY=your_groq_api_key_here

# Owner and Component
OWNER_IDS=your_owner_id_here
COMPONENT_SECRET=your_component_secret_here

# Environment
NODE_ENV=development

# Logging Configuration
LOG_LEVEL=debug
LOG=1
LOG_PRETTY=1
LOG_JSON=0
LOG_COLOR=1

# Redis Configuration
REDIS_URL=redis://127.0.0.1:6379/0
REDIS_KEY_PREFIX=void:
REDIS_LIMP_MODE=1

# Metrics and Health
METRICS_INTERVAL=30000
HEALTH_PORT=3001

# Sharding
TOTAL_SHARDS=auto

# AI Configuration
AI_ENABLED=true
AI_MODEL=llama-3.1-70b-versatile
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=2048

# AI Access Control
AI_ALLOWED_USERS=your_allowed_user_ids
AI_OWNERS=your_owner_id_here

# Rate Limits
AI_RATE_LIMIT_RPM=30
AI_RATE_LIMIT_TPM=30000
AI_RATE_LIMIT_RPD=1000
AI_RATE_LIMIT_TPD=500000

# Queue Configuration
AI_QUEUE_CONCURRENCY=5
AI_QUEUE_MAX_RETRIES=3