# Discord bot authentication
VOID_TOKEN=your_discord_bot_token
VOID_ID=your_bot_user_id
APP_ID=your_application_id

# AI provider keys (keep secret) Not used in the current implementation
GEMINI_API_KEY=your_gemini_key
GROQ_API_KEY=your_groq_key

# Access control (comma-separated Discord user IDs)
OWNER_IDS=747400496402268243
AI_ALLOWED_USERS=123
AI_OWNERS=747400496402268243

# Bot runtime
NODE_ENV=production             # production|development
TOTAL_SHARDS=auto               # auto or explicit shard count

# Logging (pino-style)
LOG_LEVEL=debug                 # debug|info|warn|error|silent
LOG=1                           # 0 to silence everything
LOG_PRETTY=1                    # pretty-print logs if installed
LOG_JSON=0                      # force JSON output
LOG_COLOR=1                     # colorize pretty output

# Redis
REDIS_URL=redis://127.0.0.1:6379/0
REDIS_KEY_PREFIX=void:
REDIS_LIMP_MODE=1               # 1 to degrade gracefully if Redis unavailable

# Metrics/health
METRICS_INTERVAL=30000          # ms between metrics pushes
HEALTH_PORT=3001                # health check port

# AI configuration
AI_ENABLED=true
AI_MODEL=llama-3.1-70b-versatile
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=2048

# AI rate limits (Groq)
AI_RATE_LIMIT_RPM=30
AI_RATE_LIMIT_TPM=30000
AI_RATE_LIMIT_RPD=1000
AI_RATE_LIMIT_TPD=500000

# AI queue
AI_QUEUE_CONCURRENCY=5
AI_QUEUE_MAX_RETRIES=3
